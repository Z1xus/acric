{
	"useTabs": true,
	"singleQuote": false,
	"trailingComma": "none",
	"printWidth": 100,
	"plugins": ["prettier-plugin-svelte"],
	"overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }],
	"arrowParens": "always",
	"endOfLine": "lf",
	"svelteStrictMode": false,
	"svelteIndentScriptAndStyle": true,
	"svelteAllowShorthand": true,
	"semi": true,
	"bracketSpacing": true,
	"bracketSameLine": false
}
